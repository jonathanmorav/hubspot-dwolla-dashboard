# Frontend Configuration (Public variables only)
# Copy this file to .env and fill in your values

# OAuth Client IDs (safe to expose in frontend)
VITE_HUBSPOT_CLIENT_ID=your_hubspot_client_id_here
VITE_DWOLLA_CLIENT_ID=your_dwolla_client_id_here

# Environment Configuration
VITE_DWOLLA_ENVIRONMENT=sandbox # or production
VITE_SESSION_TIMEOUT_MINUTES=30

# Backend API Configuration
VITE_BACKEND_API_URL=http://localhost:3001
VITE_API_KEY=your-development-key-here

# IMPORTANT: Never add client secrets here!
# Client secrets belong only in backend/.env