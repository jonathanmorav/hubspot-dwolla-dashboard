# Extension Configuration
VITE_SESSION_TIMEOUT_MINUTES=30

# OAuth Client IDs (public, used in extension)
VITE_HUBSPOT_CLIENT_ID=your_hubspot_client_id
VITE_DWOLLA_CLIENT_ID=your_dwolla_client_id
VITE_DWOLLA_ENVIRONMENT=sandbox # or production

# Backend Service Configuration
VITE_BACKEND_API_URL=http://localhost:3001
VITE_API_KEY=your-secure-api-key-here

# IMPORTANT: Client secrets should ONLY be stored in backend/.env
# Never put client secrets in this file as they would be exposed in the extension